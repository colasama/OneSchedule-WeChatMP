<van-tabbar active="{{ active }}" bind:change="onChange">
  <van-tabbar-item name="lesson" icon="clock-o">课程</van-tabbar-item>
  <van-tabbar-item name="todo" icon="eye-o">发现</van-tabbar-item>
  <van-tabbar-item name="my" icon="setting-o">设置</van-tabbar-item>
</van-tabbar>

<view style="flex-direction:column;">
  <image mode="widthFix" class="todoim" src="./todo.png"></image>
</view>

<van-card
  desc="2018级21系课表"
  title="2018级21系课表"
  thumb="{{ imageURL }}"
>
  <view slot="footer">
    <van-button size="small" bind:click="importLesson">导入</van-button>
  </view>
</van-card>

<van-card
  desc="2017级21系课表"
  title="2017级21系课表"
  thumb="{{ imageURL }}"
>
  <view slot="footer">
    <van-button size="small" bind:click="importLesson">导入</van-button>
  </view>
</van-card>